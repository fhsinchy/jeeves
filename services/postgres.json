{
    "name": "postgres",
    "image": "docker.io/postgres",
    "tag": "latest",
    "env": {
        "POSTGRES_PASSWORD": "postgres"
    },
    "ports": {
        "source": "5432/tcp",
        "destination": 5432
    },
    "volumes": {
        "name": "postgres-data",
        "destination": "/var/lib/postgresql/data"
    },
    "command": ""
}