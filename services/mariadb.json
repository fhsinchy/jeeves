{
    "name": "mariadb",
    "image": "docker.io/mariadb",
    "tag": "latest",
    "env": {
        "MYSQL_ROOT_PASSWORD": "root"
    },
    "ports": {
        "source": "3306/tcp",
        "destination": 3306
    },
    "volumes": {
        "name": "mariadb-data",
        "destination": "/var/lib/mysql"
    },
    "command": ""
}