{
    "name": "mongo",
    "image": "docker.io/mongo",
    "tag": "latest",
    "env": {
        "MONGO_INITDB_ROOT_USERNAME": "admin",
        "MONGO_INITDB_ROOT_PASSWORD": "admin"
    },
    "ports": {
        "source": "27017/tcp",
        "destination": 27017
    },
    "volumes": {
        "name": "mongo-data",
        "destination": "/data/db"
    },
    "command": "--serviceExecutor adaptive"
}